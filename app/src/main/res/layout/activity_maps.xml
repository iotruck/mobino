<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/header"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.20"
            android:orientation="vertical"
            app:layout_constraintTop_toTopOf="parent"
            android:padding="20dp"
            android:background="@drawable/card_home">
            <ImageButton
                android:id="@+id/ib_return"
                android:layout_width="23dp"
                android:layout_height="23dp"
                android:background="@mipmap/back"
                android:onClick="closeTravel"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/tv_travel_code"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="F0011SP03"
                android:textSize="30sp"
                android:textColor="@color/blue_palette"
                android:textStyle="bold"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/ib_return"
                android:layout_marginLeft="30dp"/>
            <TextView
                android:id="@+id/tv_travel_destiny"
                android:layout_width="0dp"
                app:layout_constraintWidth_percent=".80"
                android:layout_height="wrap_content"
                android:text="DESTINO: Rua Espiga de milVho, 447 - Centro, São Paulo/SP - 020304565"
                android:textColor="@color/black"
                android:layout_marginLeft="55dp"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@id/tv_travel_code"
                app:layout_constraintStart_toStartOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.63"
        app:layout_constraintTop_toBottomOf="@+id/header">
        <fragment
            xmlns:map="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".feature.maps.view.MapsActivity">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp"
                android:paddingBottom="28dp">
                <ImageButton
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:background="@mipmap/attention"
                    android:layout_marginBottom="20dp"
                    android:onClick="warningTravel"
                    map:layout_constraintStart_toStartOf="parent"
                    map:layout_constraintBottom_toTopOf="@id/ib_pause"/>
                <ImageButton
                    android:id="@+id/ib_pause"
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:background="@mipmap/pause"
                    android:onClick="pauseTravel"
                    map:layout_constraintStart_toStartOf="parent"
                    map:layout_constraintBottom_toBottomOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </fragment>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/painel_chat"
        app:layout_constraintHeight_percent="0.20"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toStartOf="@id/div_footer"
            app:layout_constraintBottom_toBottomOf="parent"
            android:onClick="goToMessages"
            >

            <ImageButton
                android:id="@+id/ib_updates"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@mipmap/chat"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/div_footer"
            android:layout_width="1dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.80"
            android:background="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@id/div_footer"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent">
            <TextView
                android:id="@+id/tv_label_time_travel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tempo_viagem"
                android:textColor="@color/orange_default"
                android:textSize="15sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toTopOf="@id/tv_time_travel"/>
            <TextView
                android:id="@+id/tv_time_travel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="01:04"
                android:textSize="40sp"
                android:textColor="@color/white"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_label_time_travel"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/div_modal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#99353535"
        android:visibility="gone"
        android:padding="40dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.60"
            android:background="@drawable/card_type_warning"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent">

            <ImageView
                android:id="@+id/iv_modal"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:src="@mipmap/logo_iotruck"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="@id/tv_modal"/>
            <TextView
                android:id="@+id/tv_modal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/iv_modal"
                app:layout_constraintBottom_toTopOf="@id/bt_modal"/>
            <Button
                android:id="@+id/bt_modal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_login"
                android:textColor="@color/white"
                android:textSize="15sp"
                android:layout_margin="30dp"
                android:onClick="btnModal"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_modal"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/div_war"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.70"
        android:visibility="gone"
        android:background="@drawable/card_warning"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <ImageButton
            android:id="@+id/ib_exit_warning"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:background="@mipmap/exit"
            android:layout_margin="20dp"
            android:onClick="warningTravel"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/div_war_cart1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintWidth_percent="0.9"
            android:background="@drawable/card_type_warning"
            android:layout_marginTop="20dp"
            android:onClick="startWarning"
            app:layout_constraintTop_toBottomOf="@+id/ib_exit_warning"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/div_war_cart1_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">
                <TextView
                    android:id="@+id/tv_war_card1_title_blue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tenho uma urgência"
                    android:textColor="#010A2C"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/tv_war_card1_title_red"/>
                <TextView
                    android:id="@+id/tv_war_card1_title_red"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" médica"
                    android:textColor="#BA4346"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/tv_war_card1_title_blue"
                    app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                android:id="@+id/tv_war_card1_contato"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tv_entrar_contato"
                android:textColor="#010A2C"
                android:textSize="12sp"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/div_war_cart1_title"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/tv_war_card1_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="193"
                android:textColor="#BA4346"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@id/tv_war_card1_contato"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <ImageView
                android:layout_width="60dp"
                android:layout_height="37dp"
                android:background="@mipmap/ambulance"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp"
                app:layout_constraintTop_toBottomOf="@id/tv_war_card1_number"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>


        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/div_war_cart2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintWidth_percent="0.9"
            android:background="@drawable/card_type_warning"
            android:layout_marginTop="20dp"
            android:onClick="startWarning"
            app:layout_constraintTop_toBottomOf="@+id/div_war_cart1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/div_war_cart2_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">
                <TextView
                    android:id="@+id/tv_war_card2_title_blue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tenho uma urgência"
                    android:textColor="#010A2C"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/tv_war_card2_title_red"/>
                <TextView
                    android:id="@+id/tv_war_card2_title_red"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" policial"
                    android:textColor="#BA4346"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/tv_war_card2_title_blue"
                    app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                android:id="@+id/tv_war_card2_contato"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tv_entrar_contato"
                android:textColor="#010A2C"
                android:textSize="12sp"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/div_war_cart2_title"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/tv_war_card2_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="190"
                android:textColor="#BA4346"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@id/tv_war_card2_contato"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@mipmap/police_light"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/tv_war_card2_number"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/div_war_cart3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintWidth_percent="0.9"
            android:background="@drawable/card_type_warning"
            android:layout_marginTop="20dp"
            android:onClick="startWarning"
            app:layout_constraintTop_toBottomOf="@+id/div_war_cart2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">
                <TextView
                    android:id="@+id/tv_war_card3_title_blue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tenho uma outra urgência"
                    android:textColor="#010A2C"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_marginTop="10dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/tv_war_card3_contato"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Entraremos em contato com o analista"
                android:textColor="#010A2C"
                android:textSize="12sp"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/tv_war_card3_title_blue"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <ImageView
                android:layout_width="28dp"
                android:layout_height="40dp"
                android:background="@mipmap/analyst"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/tv_war_card3_contato"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>






        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/div_war_final"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:visibility="gone"
        app:layout_constraintHeight_percent="0.40"
        android:background="@drawable/card_warning"
        app:layout_constraintBottom_toBottomOf="parent">
        <ImageButton
            android:id="@+id/ib_exit_warning_2"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:background="@mipmap/exit"
            android:layout_margin="20dp"
            android:onClick="warningTravel"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.60"
            app:layout_constraintWidth_percent="0.9"
            android:background="@drawable/card_type_warning"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent">
            <TextView
                android:id="@+id/div_war_final_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Estamos ligando para a opção desejada"
                android:textColor="@color/blue_palette"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/div_war_final_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/div_war_final_title">
                <TextView
                    android:id="@+id/tv_war_final_content_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Você tem"
                    android:textColor="@color/blue_palette"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/tv_war_time_cancel"/>
                <TextView
                    android:id="@+id/tv_war_time_cancel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" 00:10 "
                    android:textColor="@color/red_palette"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/tv_war_final_content_title"
                    app:layout_constraintEnd_toStartOf="@id/tv_war_final_content_title_2"/>
                <TextView
                    android:id="@+id/tv_war_final_content_title_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="segundos"
                    android:textColor="@color/blue_palette"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/tv_war_time_cancel"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="caso queira cancelar"
                    android:textColor="@color/blue_palette"
                    app:layout_constraintTop_toBottomOf="@id/tv_war_final_content_title_2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <Button
                android:layout_width="0dp"
                android:layout_height="25dp"
                app:layout_constraintWidth_percent="0.5"
                android:background="@drawable/btn_blue"
                android:layout_marginTop="20dp"
                android:text="Cancelar"
                android:textColor="@color/white"
                android:onClick="stopWarning"
                app:layout_constraintTop_toBottomOf="@id/div_war_final_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                />



        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>